<%= form_with model: @booking do |f| %>
  <%= f.label :start_date %>
  <%= f.text_field :start_date %>
  <%= f.label :end_date %>
  <%= f.text_field :end_date %>
  <%= f.submit %>
<% end %>

<div>
    <%= link_to "Delete",
    booking_path(@booking),
    data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
%>
  </div>
